<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:macro name="caster_gazebo" params="name">
    <gazebo reference="${name}_caster_link">
      <mu1 value="0.01"/>
      <mu2 value="0.01"/>
      <kp value="10000000.0"/>
      <kd value="1.0"/>
      <fdir1 value="0 1 0"/>
    </gazebo>
  </xacro:macro>
</robot>
